<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UserNotification">

    <TextView
            android:text="TextView"
            android:layout_width="288dp"
            android:layout_height="593dp"
            android:id="@+id/txtResult"
            tools:layout_editor_absoluteY="39dp" tools:layout_editor_absoluteX="61dp"/>
    <Button
            android:text="Confirmar"
            android:layout_width="114dp"
            android:layout_height="38dp"
            android:id="@+id/btnConfirmar" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="60dp" android:textAlignment="center"/>
    <Button
            android:text="Cancelar"
            android:layout_width="110dp"
            android:layout_height="38dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/btnCancelar" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toEndOf="@+id/btnConfirmar"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.478"
            android:textAlignment="center"/>
</android.support.constraint.ConstraintLayout>